{
  "name": "github-users",
  "version": "0.0.1",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-ui-router": "^0.3.1",
    "angular-toastr": "~1.7.0",
    "moment": "~2.10.6",
    "animate.css": "~3.4.0",
    "font-awesome": "~4.6.3",
    "ngInfiniteScroll": "~1.2.2",
    "bootstrap": "~3.3.6",
    "malihu-custom-scrollbar-plugin": "~3.1.3"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.0"
  },
  "overrides": {
    "font-awesome": {
      "main": [
        "css/font-awesome.css",
        "fonts/*"
      ]
    },
    "bootstrap": {
